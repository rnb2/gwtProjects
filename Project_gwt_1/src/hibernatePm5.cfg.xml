<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
                                         "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
 <session-factory name="java:/pmEntityManagerFactory_5">
	<property name="connection.datasource">java:/datasources/SecurityDS_5</property>
   
   	<property name="hibernate.current_session_context_class">thread</property>
	<property name="hibernate.hbm2ddl.auto">validate</property>
	<property name="hibernate.show_sql">false</property>
	<property name="hibernate.format_sql">false</property>
	<property name="hibernate.dialect">org.hibernate.dialect.Oracle10gDialect</property>
   
     <mapping class="com.rnb2.gwt1.data.pm.User"/>
     <mapping class="com.rnb2.gwt1.data.pm.AclPermission"/>
   	 <mapping class="com.rnb2.gwt1.data.pm.AclPermissionPK"/>
   	 <mapping class="com.rnb2.gwt1.data.pm.Application"/>
   	 <mapping class="com.rnb2.gwt1.data.pm.Permission"/>
   	 <mapping class="com.rnb2.gwt1.data.pm.Profile"/>
   	 <mapping class="com.rnb2.gwt1.data.pm.ProfileUser"/>
   
   	
   </session-factory>
   
</hibernate-configuration>
